{
    "name": "logarithm-wallet",
    "version": "0.0.2",
    "description": "Logarithm Token Wallet from Logarithm Network",
    "author": "Logarithm Network <info@Logarithm.network>",
    "main": "main.js",
    "scripts": {
        "start": "electron .",
        "pack": "electron-builder --dir",
        "dist": "build -wl"
    },
    "build": {
        "appId": "com.wallet.logarithm",
        "productName": "Logarithm Token Wallet",
        "mac": {
            "category": "wallet.wallet.category.type"
        },
        "linux": {
            "target": [
                "deb"
            ]
        },
        "win": {
            "icon": "build/icon.ico"
        }
    },
    "repository": "https://github.com/electron/electron-quick-start",
    "keywords": [
        "logarithm",
        "lgr",
        "lgr token",
        "erc20",
        "logarithm-wallet"
    ],
    "license": "MIT",
    "devDependencies": {
        "electron": "^2.0.1",
        "electron-builder": "^19.11.1",
        "electron-packager": "^12.0.2"
    },
    "dependencies": {
        "ajv": "^6.5.0",
        "clipboardy": "^1.1.4",
        "ethers": "^2.1.1"
    }
}
